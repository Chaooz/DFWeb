{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build_bootstrap",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/DFWeb.FR.BootStrap/DFWeb.FR.BootStrap.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "rebuild_bootstrap",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "--no-incremental",
                "${workspaceFolder}/DFWeb.FR.BootStrap/DFWeb.FR.BootStrap.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish_bootstrap",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/DFWeb.FR.BootStrap/DFWeb.FR.BootStrap.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch_bootstrap",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/DFWeb.FR.BootStrap/DFWeb.FR.BootStrap.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build_tailwind",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/DFWeb.FR.Tailwind/DFWeb.FR.Tailwind.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        }
   ]
}